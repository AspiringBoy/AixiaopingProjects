<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomToolbar">
        <attr name="leftIv" format="reference" />
        <attr name="rightIv" format="reference" />
        <attr name="titleSize" format="dimension|reference" />
        <attr name="titleColor" format="color|reference" />
        <attr name="titleName" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="IndexBar">
        <attr name="textSize" format="dimension" />
        <attr name="pressBackground" format="color" />
    </declare-styleable>

    <declare-styleable name="MyRadiobutton">
        <attr name="drawableTopWidth" format="dimension" />
        <attr name="drawableTopHeight" format="dimension" />
        <attr name="MydrawableTop" format="reference" />
    </declare-styleable>

    <declare-styleable name="RoundImgView">
        <attr name="type" format="integer"/>
        <attr name="roundRadios" format="dimension" />
    </declare-styleable>
</resources>